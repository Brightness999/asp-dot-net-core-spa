{
  "name": "aspnetnetcore",
  "version": "0.0.0",
  "private": true,
  "keywords": [
    "aspnetcore",
    "entityframework core",
    "angular",
    "oauth",
    "angular-cli",
    "typescript"
  ],
  "repository": "git+https://github.com/asadsahi/AspNetCoreSpa.git",
  "author": "Asad Sahi <asadazmat@gmail.com>",
  "scripts": {
    "ng": "ng",
    "clean:install": "rimraf node_modules & yarn",
    "start": "ng serve --hmr",
    "build": "ng build --configuration production",
    "build:ssr": "npm run build --project=ssr",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "compodoc": "./node_modules/.bin/compodoc -p tsconfig.json",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats.json"
  },
  "dependencies": {
    "@angular/animations": "12.2.13",
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/forms": "12.2.13",
    "@angular/localize": "^12.2.13",
    "@angular/platform-browser": "12.2.13",
    "@angular/platform-browser-dynamic": "12.2.13",
    "@angular/platform-server": "12.2.13",
    "@angular/router": "12.2.13",
    "@microsoft/signalr": "5.0.2",
    "@microsoft/signalr-protocol-msgpack": "5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^10.0.0",
    "ag-grid-angular": "^25.3.0",
    "ag-grid-community": "^25.3.0",
    "aspnet-prerendering": "3.0.1",
    "bootstrap": "4.6.0",
    "date-fns": "2.16.1",
    "lodash": "4.17.20",
    "oidc-client": "1.10.1",
    "preboot": "8.0.0",
    "rxjs": "6.6.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.13",
    "@angular/cli": "12.2.13",
    "@angular/language-service": "12.2.13",
    "@angular/pwa": "0.1101.1",
    "@angular/service-worker": "12.2.13",
    "@compodoc/compodoc": "1.1.11",
    "@types/jest": "^26.0.20",
    "@types/node": "12.12.21",
    "codelyzer": "^6.0.1",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "protractor": "7.0.0",
    "ts-node": "8.5.4",
    "tslint": "6.1.3",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "4.4.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    }
  },
  "license": "MIT"
}
